<script type="text/javascript">
var i = 0;
var txt = 'Generally speaking, money has their central control department. In Korea, Bank of Korea is in charge of production and supply of money, also they control the supply amount of currencies, but Bitcoin does not have such department. In other words, there is no department which in charges their supply. However, everyone is able to produce Bitcoin which can be done with the high-performance computer that can solve the mathematical problem and as a reward Bitcoins are given. Process of Bitcoin productions are also called as mining and miners are the one who produces Bitcoin. Thus, miners are the currency issuer in Bitcoin world.';
var speed = 50;

function typeWriter() {
  if (i < txt.length) {
    document.getElementById("demo").innerHTML += txt.charAt(i);
    i++;
    setTimeout(typeWriter, speed);
  }
}
$(document).ready(function(){

  // First
  $("#FirstCode").click(function(){
    $(this).toggleClass("disappear");
    $('#really').toggleClass("disappear");
    $('#SecondCode').toggleClass("show");
    $("#Lights-1").toggleClass("show");
    $("#mining").toggleClass("show");
  // Second
      $("#SecondCode").click(function(){
    $(this).toggleClass("disappear");
    $('#ThirdCode').toggleClass("show");
      $("#Lights-2").toggleClass("show");
  // Third
    $("#ThirdCode").click(function(){
  $(this).toggleClass("disappear");
  $('#FourthCode').toggleClass("show");
    $("#Lights-3").toggleClass("show");
  // Fifith
  $("#FourthCode").click(function(){
$(this).toggleClass("disappear");
$('#FifthCode').toggleClass("show");
  $("#Lights-4").toggleClass("show");




});
});
});
});
});

</script>


